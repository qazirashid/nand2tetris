/** 
 * Multiplexor:
 * out = a if sel == 0
 *       b otherwise
 */

CHIP Mux {
    IN a, b, sel;
    OUT out;

    PARTS:
    // Put your code here:
    Not(in = sel, out = selprime);
    Nand(a=a, b=selprime, out = c);
    Nand(a=b, b=sel, out=d);
    Nand(a=c, b=d, out=out);
 }